<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="controllers.ExerciseLogController">
    <VBox prefWidth="400" alignment="CENTER">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <VBox fx:id="exerciseVBox" spacing="10" alignment="CENTER">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
        <HBox spacing="10" alignment="CENTER">
            <TextField fx:id="exerciseTextField" promptText="Exercise"
                       prefWidth="90"/>
            <Button text="Add" onAction="#addExercise"/>
        </HBox>
    </VBox>
</ScrollPane>
